{
  "name": "FIX.YTM - The ultimate YouTube Music extension",
  "manifest_version": 3,
  "version": "1.0.8",
  "description": "Transcend the limits of your YouTube Music by adding many new features & access to authorised actions previously unavailable on YTM",
  "content_scripts": [{
    "matches": ["https://music.youtube.com/*"],
    "css": ["fixytm-react.css"],
    "js": ["fixytm-react.js"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "FIXYTM_Main_16px.png",
      "32": "FIXYTM_Main_32px.png",
      "48": "FIXYTM_Main_48px.png",
      "128": "FIXYTM_Main_128px.png"
    }
  },
  "icons": {
    "16": "FIXYTM_Main_16px.png",
    "32": "FIXYTM_Main_32px.png",
    "48": "FIXYTM_Main_48px.png",
    "128": "FIXYTM_Main_128px.png"
  }
}
